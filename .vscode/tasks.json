{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Copy VLC Lua",
			"type": "shell",
			"command": "python",
			"args": [
				"copy_vlc_lua.py"
			],
			"options": {
				"cwd": "${workspaceFolder}/user"
			}
		},
		{
			"label": "Run VLC",
			"type": "process",
			"command": "${workspaceFolder}/vlc/vlc.exe",
			"options": {
				"cwd": "${workspaceFolder}/vlc"
			}
		},
		{
			"type": "cmake",
			"label": "CMake: build",
			"command": "build",
			"targets": [
				"all"
			],
			"preset": "${command:cmake.activeBuildPresetName}",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": [],
			"detail": "CMake template build task"
		}
	]
}